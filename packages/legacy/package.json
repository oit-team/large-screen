{
  "name": "@oit/legacy",
  "private": true,
  "module": "./dist/legacy.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite --host",
    "build": "vite build",
    "build:dev": "vite build -w",
    "lint": "eslint \"**/*.{vue,ts,js,json}\" --fix",
    "fix:sass": "sass-migrator division node_modules/**/*.scss",
    "prepare": "npm run fix:sass"
  },
  "dependencies": {
    "@unocss/reset": "^0.46.2",
    "@vueuse/core": "^9.0.0",
    "axios": "^1.1.3",
    "core-js": "^3.24.0",
    "crypto-js": "^4.1.1",
    "element-ui": "^2.15.10",
    "lodash-es": "^4.17.21",
    "vue": "^2.7.8",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.5.4",
    "vuex": "^3"
  },
  "devDependencies": {
    "@iconify/json": "^2.1.83",
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue2": "^1.1.2",
    "@vitejs/plugin-vue2-jsx": "^1.0.3",
    "autoprefixer": "^10.4.13",
    "less": "^4.1.3",
    "postcss": "^8.4.18",
    "sass": "^1.55.0",
    "sass-migrator": "^1.7.1",
    "tailwindcss": "^3.2.2",
    "unocss": "^0.46.2",
    "unplugin-auto-import": "^0.10.1",
    "unplugin-icons": "^0.14.7",
    "unplugin-vue-components": "^0.21.2",
    "vite": "^3.0.3",
    "vite-plugin-eslint": "^1.8.1",
    "vue-template-compiler": "^2.7.8",
    "vue-template-es2015-compiler": "^1.9.1"
  },
  "eslintConfig": {
    "rules": {
      "vue/custom-event-name-casing": "off",
      "@typescript-eslint/no-unused-vars": "warn",
      "no-console": [
        "warn",
        {
          "allow": [
            "warn",
            "error",
            "info"
          ]
        }
      ],
      "eqeqeq": "warn",
      "vue/eqeqeq": "warn",
      "@typescript-eslint/no-this-alias": "warn",
      "no-tabs": "warn",
      "no-self-assign": "warn",
      "prefer-regex-literals": "warn",
      "vue/no-unused-vars": "warn",
      "vue/no-irregular-whitespace": "warn",
      "no-irregular-whitespace": "warn",
      "vue/component-name-in-template-casing": ["error", "kebab-case", {
        "registeredComponentsOnly": false
      }]
    }
  }
}
